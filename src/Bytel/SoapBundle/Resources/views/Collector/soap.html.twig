{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {# le contenu de la barre d'outils de d√©buggage web #}
{% endblock %}

{% block head %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/bytelsoap/css/profiler.min.css') }}" />
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="{{ asset('/bundles/bytelsoap/images/XML-SOAP.jpg') }}" alt="" /></span>
        <strong>SOAP</strong>
        <span class="count">
            <span>{{ collector.data|length }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    {% for index,call in collector.data %}
        {% include 'BytelSoapBundle:Collector:call.html.twig' with { 'call': call, 'index': index } %}
    {% endfor %}
    
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
{% endblock %}